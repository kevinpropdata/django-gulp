#!/bin/sh

env_dir=$3

whitelist_regex=${2:-''}
blacklist_regex=${3:-'^(PATH|GIT_DIR|CPATH|CPPATH|LD_PRELOAD|LIBRARY_PATH)$'}
if [ -d "$env_dir" ]; then
  for e in $(ls $env_dir); do
    export "$e=$(cat $env_dir/$e)";
  done
fi

build_dir=$1
pwd
./node_modules/.bin/gulp build
mv static/build $build_dir/static/
